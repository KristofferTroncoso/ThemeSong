  "scripts": {
    "start": "node tasks/webserver.js",
    "build": "node tasks/build.js",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "zip": "pnpm run build && web-ext build --source-dir ./builds/chrome-build --artifacts-dir zip/chrome-zip --overwrite-dest",
